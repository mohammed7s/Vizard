{
  "name": "@vizard/wallet",
  "version": "0.1.0",
  "description": "MetaMask-powered Aztec wallet - Browser PXE with MetaMask signing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
